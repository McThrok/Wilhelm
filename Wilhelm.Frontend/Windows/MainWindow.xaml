<Window x:Class="Wilhelm.Frontend.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wilhelm.Frontend"
        xmlns:viewSigning="clr-namespace:Wilhelm.Frontend.Views.Signing"
        xmlns:viewModels="clr-namespace:Wilhelm.Frontend.ViewModels.Signing"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Wilhelm" Height="600" Width="850"
        Background="#ddd8c9">

    <Window.Resources>

        <DataTemplate DataType="{x:Type viewModels:SignViewModel}">
            <viewSigning:SignView/>
        </DataTemplate>
    </Window.Resources>

    <ContentControl Name="MainContent"/>

</Window>
