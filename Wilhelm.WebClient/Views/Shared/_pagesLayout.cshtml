<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>

    <div>@ViewBag.Title</div>
    <div>@Html.ActionLink("Home", "HomePage", "Pages", new { userId = ViewBag.UserId },null)</div>
    <div>@Html.ActionLink("Tasks", "TasksPage", "Pages", new { userId = ViewBag.UserId },null)</div>
    <div>@Html.ActionLink("Groups", "GroupsPage", "Pages", new { userId = ViewBag.UserId }, null)</div>
    <div>@Html.ActionLink("Archive", "ArchivePage", "Pages", new { userId = ViewBag.UserId },null)</div>
    <div>@Html.ActionLink("Report", "ReportPage", "Pages", new { userId = ViewBag.UserId }, null)</div>

    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
